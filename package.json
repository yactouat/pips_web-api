    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "dotenv": "^16.0.3",
    "jest": "^29.3.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^3.3.0",
    "@google-cloud/storage": "^6.9.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
